{
  "buildCommand": "cd treinos && npm install && npm run build",
  "outputDirectory": "treinos/dist",
  "installCommand": "npm install",
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096"
  },
  "rewrites": [
    {
      "source": "/treinos/:path*",
      "destination": "/treinos/dist/:path*"
    },
    {
      "source": "/treinos",
      "destination": "/treinos/dist/index.html"
    }
  ]
}