{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
  },
  "rewrites": [
    {
      "source": "/treinos/:path*",
      "destination": "/dist/:path*"
    },
    {
      "source": "/treinos",
      "destination": "/dist/index.html"
    }
  ]
}
